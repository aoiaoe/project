<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cz.springbootmybatissourcecode.mapper.StudentMapper">

    <insert id="insertStudent" parameterType="com.cz.springbootmybatissourcecode.entity.Student">
        insert into student(name, age, sex) values (#{name}, #{age}, #{sex})
    </insert>
</mapper>
